<div class="grid_3 box <%= (this.index % 3 == 0) ? 'alpha' : (this.index % 3 == 2) ? 'omega' : '' %>">
  <div class="product_item" id="product-<%=this.product.id%>">
    <h3>
      <a href="#products/show&id=<%=this.product.id%>"><%=this.product.name%></a>
    </h3>
    <div class="image"></div>
    <p class="attribute">
      <span class="label">
        Rank:
      </span>
      <span class="price">
        <%=this.product.price%>
      </span>
    </p>
    <div class="clear"></div>
  </div>
</div>
<% if (this.index % 3 == 2) { %>
       <div class="clear"></div>
<% } %>

